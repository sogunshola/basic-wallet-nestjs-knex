{
  "name": "nest-template",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "knex:migrate:latest": "knex --knexfile dist/knexfile.js migrate:latest --env development",
    "knex:migrate:rollback": "knex --knexfile dist/knexfile.js migrate:rollback --env development"
  },
  "dependencies": {
    "@mithleshjs/knex-nest": "^2.2.0",
    "@nestjs-modules/mailer": "1.8.1",
    "@nestjs/bull": "^0.6.2",
    "@nestjs/common": "^9.2.0",
    "@nestjs/core": "^9.2.0",
    "@nestjs/event-emitter": "^1.0.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.2.0",
    "@nestjs/platform-socket.io": "^9.2.0",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "^6.1.3",
    "@nestjs/websockets": "^9.2.0",
    "@types/cache-manager": "^4.0.2",
    "@types/faker": "^6.6.9",
    "app-root-path": "^3.1.0",
    "axios": "^1.5.0",
    "bcrypt": "^5.0.0",
    "bull": "^4.6.1",
    "cache-manager": "^5.1.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.3",
    "env-var": "^7.1.1",
    "express-basic-auth": "^1.2.1",
    "faker": "5.5.3",
    "find-config": "^1.0.0",
    "firebase-admin": "^11.3.0",
    "knex": "^2.5.1",
    "memory-cache": "^0.2.0",
    "moment": "^2.29.1",
    "multer": "^1.4.5-lts.1",
    "mysql": "^2.18.1",
    "nestjs-role-protected": "^0.2.1",
    "nodemailer-mailgun-transport": "^2.1.3",
    "otp-generator": "^4.0.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "redis": "^4.0.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.7",
    "slugify": "^1.4.6",
    "snappy": "^7.2.2",
    "socket.io": "^4.4.1",
    "swagger-ui-express": "^4.1.5",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.5",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.2.0",
    "@types/express": "^4.17.8",
    "@types/jest": "^29.2.3",
    "@types/memory-cache": "^0.2.2",
    "@types/node": "^18.11.9",
    "@types/redis": "^4.0.11",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "knex-mock-client": "^2.0.0",
    "mock-knex": "^0.4.12",
    "prettier": "^2.1.2",
    "supertest": "^6.0.0",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.0.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
